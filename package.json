{
  "name": "@onlybond/crate",
  "version": "1.0.0",
  "description": "A virtual environment for Node.js â€” like Python's venv, but for npm, pnpm, yarn, and bun",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "crate": "./bin/crate"
  },
  "files": [
    "dist/",
    "bin/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "ts-node src/cli.ts",
    "build": "tsc",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm test && pnpm lint && pnpm type-check",
    "audit": "pnpm audit --audit-level moderate",
    "audit:fix": "pnpm audit --fix"
  },
  "keywords": [
    "node",
    "modules",
    "packages",
    "dependencies",
    "environment",
    "crate",
    "virtual-environment",
    "package-manager",
    "npm",
    "pnpm",
    "yarn",
    "bun",
    "cli",
    "tool"
  ],
  "author": {
    "name": "Crate Contributors",
    "url": "https://github.com/onlybond/crate"
  },
  "license": "MIT",
  "homepage": "https://github.com/onlybond/crate#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/onlybond/crate.git"
  },
  "bugs": {
    "url": "https://github.com/onlybond/crate/issues"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.10.0",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "7.7.0",
    "@typescript-eslint/parser": "7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "execa": "^9.6.0"
  }
}
